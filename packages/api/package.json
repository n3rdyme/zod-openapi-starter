{
  "name": "@local/api",
  "private": true,
  "sideEffects": true,
  "version": "1.0.0",
  "main": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "scripts": {
    "build": "yarn build:code && yarn build:openapi && yarn build:orval",
    "build:code": "tsc",
    "build:openapi": "node ./dist/generator/generate.mjs ./dist/openapi.json",
    "build:orval": "NODE_NO_WARNINGS=1 orval orval.config.js",
    "lint": "eslint --fix --max-warnings=0 ./src",
    "for-each": "yarn"
  },
  "devDependencies": {
    "@asteasolutions/zod-to-openapi": "^7.2.0",
    "json-schema-faker": "^0.5.6",
    "orval": "^7.2.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vitest": "^2.1.3"
  },
  "dependencies": {
    "zod": "^3.23.8"
  }
}

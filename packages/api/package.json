{
  "name": "@local/api",
  "private": true,
  "sideEffects": true,
  "version": "1.0.0",
  "main": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "scripts": {
    "clean": "rm -rf .turbo && rm -rf dist && rm -rf coverage",
    "build": "yarn build:code && yarn build:openapi && yarn build:orval && yarn build:proto",
    "build:code": "tsc",
    "build:openapi": "node ./dist/generator/generate.mjs ./dist/openapi.json",
    "build:orval": "NODE_NO_WARNINGS=1 orval -c orval.config.js",
    "build:proto": "../../proto/build.sh",
    "lint": "eslint --fix --max-warnings=0 ./src",
    "for-each": "yarn"
  },
  "devDependencies": {
    "@asteasolutions/zod-to-openapi": "^8.4.0",
    "json-schema-faker": "^0.5.9",
    "orval": "^8.2.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "zod": "^4.3.6"
  }
}
